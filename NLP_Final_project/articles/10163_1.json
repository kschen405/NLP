930\u094b\u0938\u0947 \u0917\u090f \u092d\u093e\u0947\u091c\u0928 \u0915\u094b", "\u092a\u094d\u0930\u0935\u093e\u0938\u093f\u092f\u094b\u0902 \u0928\u0947 \u0932\u093e\u0924 \u092e\u093e\u0930\u0915\u0930 \u092b\u0947\u0902\u0915 \u0926\u093f\u092f\u093e\u0964 \u092c\u0940\u0921\u0940\u0913 \u0928\u0947 \u0915\u0939\u093e \u0926\u0930\u094d\u091c \u0915\u0940 \u091c\u093e\u090f\u0917\u0940 \u092a\u094d\u0930\u093e\u0925\u092e\u093f\u0915\u0940", "\u0918\u091f\u0928\u093e \u0915\u093e \u0935\u0940\u0921\u093f\u092f\u094b \u0935\u093e\u092f\u0930\u0932\u0964", "\u092e\u0927\u0941\u092c\u0928\u0940, \u091c\u0947\u090f\u0928\u090f\u0928\u0964 \u092e\u0927\u0935\u093e\u092a\u0941\u0930 \u092a\u094d\u0930\u0916\u0902\u0921 \u0915\u0947 \u092e\u0927\u094d\u092f \u0935\u093f\u0926\u094d\u092f\u093e\u0932\u092f, \u0938\u093e\u0939\u0930\u0918\u093e\u091f \u0915\u094d\u0935\u093e\u0930\u0902\u091f\u093e\u0907\u0928 \u0938\u0947\u0902\u091f\u0930", "\u092a\u0930 \u092a\u0930\u094b\u0938\u0947 \u0917\u090f \u092d\u094b\u091c\u0928 \u0915\u094b \u092a\u094d\u0930\u0935\u093e\u0938\u093f\u092f\u094b\u0902 \u0928\u0947 \u092b\u0947\u0902\u0915 \u0926\u093f\u092f\u093e\u0964 \u091f\u0947\u092c\u0932 \u092a\u0930 \u0930\u0916\u0947 \u0917\u090f \u092d\u094b\u091c\u0928 \u092a\u0930 \u0932\u093e\u0924 \u092d\u0940", "\u092e\u093e\u0930\u0940\u0964 \u0938\u093e\u0925 \u0939\u0940 \u0916\u093e\u0928\u093e \u092a\u0930\u094b\u0938\u0928\u0947 \u0935\u093e\u0932\u0940\u0902 \u092e\u0939\u093f\u0932\u093e \u0930\u0938\u094b\u0907\u092f\u094b\u0902 \u0915\u0947 \u0938\u093e\u0925 \u0926\u0941\u0930\u094d\u0935\u094d\u092f\u0935\u0939\u093e\u0930 \u0915\u093f\u092f\u093e\u0964 \u0918\u091f\u0928\u093e", "\u0915\u094b \u0932\u0947\u0915\u0930 \u0938\u0947\u0902\u091f\u0930 \u0915\u0947 \u092a\u094d\u0930\u092d\u093e\u0930\u0940 \u092a\u094d\u0930\u0927\u093e\u0928\u093e\u0927\u094d\u092f\u093e\u092a\u0915 \u0939\u0930\u093f\u0926\u0947\u0935 \u092a\u094d\u0930\u0938\u093e\u0926 \u0928\u0947 \u092c\u0940\u0921\u0940\u0913 \u092c\u0948\u092d\u0935 \u0915\u0941\u092e\u093e\u0930", "\u0938\u0947 \u0906\u0917\u0947 \u0915\u0940 \u0915\u093e\u0930\u094d\u0930\u0935\u093e\u0908 \u0915\u093e \u0906\u0917\u094d\u0930\u0939 \u0915\u093f\u092f\u093e \u0939\u0948\u0964", "\u092f\u0939\u093e\u0902 \u0926\u0947\u0916\u093f\u090f \u0935\u093e\u092f\u0930\u0932 \u0935\u0940\u0921\u093f\u092f\u094b", "\u0935\u0939\u0940\u0902 \u0918\u091f\u0928\u093e \u0915\u0940 \u0938\u0942\u091a\u0928\u093e \u092a\u0930 \u0938\u0926\u0932\u0932 \u092a\u0939\u0941\u0902\u091a\u0947 \u0938\u093e\u0939\u0930\u0918\u093e\u091f \u0925\u093e\u0928\u093e\u0927\u094d\u092f\u0915\u094d\u0937 \u0938\u0941\u0930\u0947\u0928\u094d\u0926\u094d\u0930 \u092a\u093e\u0938\u0935\u093e\u0928", "\u092a\u094d\u0930\u0935\u093e\u0938\u093f\u092f\u094b\u0902 \u0915\u0940 \u0917\u0924\u093f\u0935\u093f\u0927\u093f \u0915\u094b \u0917\u0932\u0924 \u0920\u0939\u0930\u093e\u0924\u0947 \u0939\u0941\u090f \u092c\u0940\u0921\u0940\u0913 \u0915\u094b \u0918\u091f\u0928\u093e \u0938\u0947 \u0905\u0935\u0917\u0924 \u0915\u0930\u093e\u092f\u093e\u0964 \u092c\u0940\u0921\u0940\u0913", "\u0928\u0947 \u092c\u0924\u093e\u092f\u093e \u0915\u093f \u092e\u0939\u093f\u0932\u093e \u0930\u0938\u094b\u0907\u092f\u094b\u0902 \u0915\u0947 \u0938\u093e\u0925 \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0926\u0941\u0930\u094d\u0935\u094d\u092f\u0935\u0939\u093e\u0930 \u0905\u092e\u093e\u0928\u0935\u0940\u092f \u0939\u0948\u0964 \u092e\u0939\u093f\u0932\u093e", "\u0930\u0938\u094b\u0907\u092f\u093e \u091c\u093e\u0928 \u091c\u094b\u0916\u093f\u092e \u092e\u0947\u0902 \u0921\u093e\u0932\u0915\u0930 \u092a\u094d\u0930\u0935\u093e\u0938\u093f\u092f\u094b\u0902 \u0915\u094b \u0916\u093e\u0928\u093e \u0916\u093f\u0932\u093e \u0930\u0939\u0940 \u0939\u0948\u0902\u0964 \u0907\u0938\u0915\u0947 \u092c\u093e\u0935\u091c\u0942\u0926 \u0907\u0938", "\u0924\u0930\u0939 \u0915\u093e \u0935\u094d\u092f\u0935\u0939\u093e\u0930 \u0905\u0928\u0941\u091a\u093f\u0924 \u0939\u0948\u0964 \u092e\u093e\u092e\u0932\u0947 \u0915\u0940 \u091c\u093e\u0902\u091a \u0915\u0930 \u0926\u094b\u0937\u093f\u092f\u094b\u0902 \u0915\u0947 \u0916\u093f\u0932\u093e\u092b \u092a\u094d\u0930\u093e\u0925\u092e\u093f\u0915\u0940 \u0926\u0930\u094d\u091c", "\u0915\u0940 \u091c\u093e\u090f\u0917\u0940\u0964", "\u092e\u093e\u0932\u0942\u092e \u0939\u094b \u0915\u093f \u092e\u0927\u094d\u092f \u0935\u093f\u0926\u094d\u092f\u093e\u0932\u092f \u0938\u093e\u0939\u0930\u0918\u093e\u091f \u0915\u094d\u0935\u093e\u0930\u0902\u091f\u093e\u0907\u0928 \u0938\u0947\u0902\u091f\u0930 \u092a\u0930 25 \u092a\u094d\u0930\u0935\u093e\u0938\u0940 \u092e\u091c\u0926\u0942\u0930", "\u0915\u094d\u0935\u093e\u0930\u0902\u091f\u093e\u0907\u0928 \u092e\u0947\u0902 \u0939\u0948\u0902\u0964 \u090f\u092e\u0921\u0940\u090f\u092e \u0915\u0947 \u0930\u0938\u094b\u0907\u092f\u094b\u0902 \u0915\u0947 \u0926\u094d\u0935\u093e\u0930\u093e \u0907\u0928 \u092a\u094d\u0930\u0935\u093e\u0938\u0940 \u092e\u091c\u0926\u0942\u0930\u094b\u0902 \u0915\u094b \u092d\u094b\u091c\u0928", "\u092c\u0928\u093e\u0915\u0930 \u0916\u093f\u0932\u093e\u092f\u093e \u091c\u093e\u0924\u093e \u0939\u0948\u0964 \u0938\u094b\u092e\u0935\u093e\u0930 \u0926\u094b\u092a\u0939\u0930 \u092e\u0939\u093f\u0932\u093e \u0930\u0938\u094b\u0907\u092f\u093e \u0925\u093e\u0932\u0940 \u092e\u0947\u0902 \u0916\u093e\u0928\u093e \u092a\u0930\u094b\u0938 \u0915\u0930 \u092c\u0947\u0902\u091a", "\u092a\u0930 \u0930\u0916 \u0926\u0940\u0902\u0964 \u092e\u0917\u0930, \u092a\u094d\u0930\u0935\u093e\u0938\u0940 \u092e\u091c\u0926\u0942\u0930\u094b\u0902 \u092e\u0947\u0902 \u0938\u0947 \u0915\u0941\u091b \u0928\u0947 \u092c\u093f\u0928\u093e \u0938\u094b\u0936\u0932 \u0921\u093f\u0938\u094d\u091f\u0947\u0902\u0938\u093f\u0902\u0917 \u092c\u0928\u093e\u090f", "\u092c\u0948\u0920\u0915\u0930 \u0916\u093e\u0928\u093e \u0916\u093f\u0932\u093e\u0928\u0947 \u0915\u094b \u0915\u0939\u093e\u0964 \u0930\u0938\u094b\u0907\u092f